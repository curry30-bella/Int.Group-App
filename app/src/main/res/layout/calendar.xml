<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/white"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tvYear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:gravity="center"
        android:padding="10dp"
        android:text="2023 April"
        android:textColor="@android:color/white" />

    <edu.northeastern.numad23sp_zihaoluo_shizhong_zhenliu_project.InnerGridView
        android:id="@+id/gvWeek"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:numColumns="7"
        tools:ignore="MissingClass"></edu.northeastern.numad23sp_zihaoluo_shizhong_zhenliu_project.InnerGridView>

    <edu.northeastern.numad23sp_zihaoluo_shizhong_zhenliu_project.InnerGridView
        android:id="@+id/gvDate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:numColumns="7"></edu.northeastern.numad23sp_zihaoluo_shizhong_zhenliu_project.InnerGridView>
</LinearLayout>